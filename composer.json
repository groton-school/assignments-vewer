{
    "name": "groton-school/major-assignments",
    "version": "1.0",
    "description": "Return an individual student's list of assignments",
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "GrotonSchool\\": "src/",
            "GrotonSchool\\OAuth2\\Client\\": "external/oauth2-sky-api/src/",
            "Battis\\PDOFactory\\": "external/pdo-factory/src/",
            "Battis\\Psr\\LTI\\": "external/psr-lti/src/"
        }
    },
    "authors": [
        {
            "name": "Seth Battis",
            "email": "sbattis@groton.org"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "vlucas/phpdotenv": "^5.3",
        "php-di/php-di": "6.*",
        "slim/slim": "4.*",
        "slim/psr7": "^1.5",
        "slim/http": "^1.2",
        "league/oauth2-client": "^2.6",
        "tuupola/cors-middleware": "^1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "scripts": {
        "post-autoload-dump": [
            "\\GrotonSchool\\Install::run"
        ]
    }
}
